function [ samplePlot ] = PlotSample(x)
hold on;

[mu, sigma] = sge(x);
numberOfCircles = 3;
numSamples = length(x);

scatter(x(:,1), x(:,2), '.');


pointsOutsideCircle = zeros(numSamples, numberOfCircles);

xCenter = mu(1);
yCenter = mu(2);

angle = linspace(0, 2*pi, 1000);




for k=1:numberOfCircles
    
    for i=1:numSamples
        outside = ;
    
       
    end
    
    xCoord = k*sigma * cos(angle) + xCenter;
    yCoord = k*sigma * sin(angle) + yCenter;
   
    plot(xCoord, yCoord);
    
end

%samplePlot;


end

